{
  "name": "@kerunix/nuxt-init",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.15.0",
  "description": "CLI tool to scaffold a Nuxt application with opinionated defaults",
  "author": "",
  "license": "MIT",
  "keywords": [
    "nuxt",
    "cli",
    "scaffold",
    "tailwind",
    "shadcn",
    "vue"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "bin": {
    "nuxt-init": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "tinyexec": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@types/node": "catalog:",
    "bumpp": "catalog:",
    "c12": "catalog:",
    "citty": "catalog:",
    "consola": "catalog:",
    "eslint": "catalog:",
    "magicast": "catalog:",
    "nypm": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  }
}
