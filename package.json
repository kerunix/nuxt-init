{
  "name": "@kerunix/nuxt-init",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.15.0",
  "description": "CLI tool to scaffold a Nuxt application with opinionated defaults",
  "author": "",
  "license": "MIT",
  "keywords": [
    "nuxt",
    "cli",
    "scaffold",
    "tailwind",
    "shadcn",
    "vue"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "bin": {
    "nuxt-init": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "tinyexec": "^1.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@types/node": "^24.5.2",
    "bumpp": "^10.2.3",
    "c12": "^3.3.0",
    "citty": "^0.1.6",
    "consola": "^3.4.2",
    "eslint": "^9.35.0",
    "magicast": "^0.3.5",
    "nypm": "^0.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  }
}
